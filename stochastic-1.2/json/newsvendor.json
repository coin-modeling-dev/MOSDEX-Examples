{
  "SYNTAX": "MOSDEX/MOSDEX v1-2/MOSDEXSchemaV1-2.json",
  "newsvendor": {
    "CLASS": "PROBLEM",
    "HEADING": {
      "DESCRIPTION": [
        "Newsvendor problem.",
        "Canonical stochastic program.",
        "Shows use of database technology to bring in sales data.",
        "MOSDEX 1-2 Syntax"
      ],
      "VERSION": [
        "1-2"
      ],
      "AUTHOR": [
        "Alan J. King (kingaj@us.ibm.com)"
      ],
      "NOTICES": [
        "Copyright Â© 2019 IBM "
      ]
    },
    "budget": {
      "CLASS": "DATA",
      "TYPE": "INPUT",
      "SCHEMA": {
        "budget": "DOUBLE"
      },
      "INSTANCE": [ [1000.0] ]
    },
    "unit_cost": {
      "CLASS": "DATA",
      "TYPE": "INPUT",
      "SCHEMA": {
        "Name": "STRING",
        "title": "STRING",
        "unit_cost": "DOUBLE"
      },
      "KEYS": [ "title" ],
      "FIELDS": [ "Name","title", "unit_cost" ],
      "INSTANCE": [
        [ "cost","Wall_Street_Journal", 1.75 ],
        [ "cost","New_York_Times",      1.50 ]
      ]
    },
    "order": {
      "CLASS": "VARIABLE",
      "TYPE": "INTEGER",
      "SCHEMA": {
        "Name": "STRING",
        "title": "STRING",
        "Column": "STRING",
        "LowerBound": "INTEGER",
        "UpperBound": "INTEGER",
        "primal": "INTEGER",
        "Type": "STRING"
      },
      "KEYS": ["title"],
      "FIELDS": ["Name", "title", "Column","LowerBound", "UpperBound", "primal"],
      "INSTANCE": [
        ["order", "Wall_Street_Journal", "order_WSJ", 0, 1000, null, "INTEGER"],
        ["order", "New_York_Times",      "order_NYT", 0, 1000, null, "INTEGER"]
      ]
    },
    "sold": {
      "CLASS": "VARIABLE",
      "TYPE": "INTEGER",
      "SCHEMA": {
        "Name": "STRING",
        "title": "STRING",
        "Column": "STRING",
        "LowerBound": "INTEGER",
        "UpperBound": "INTEGER",
        "primal": "INTEGER",
        "Type": "STRING"
      },
      "KEYS": ["title"],
      "FIELDS": ["Name", "title", "LowerBound", "UpperBound", "primal"],
      "INSTANCE": [
        ["sold", "Wall_Street_Journal", "sold_WSJ", 0, 1000, null, "INTEGER"],
        ["sold", "New_York_Times",      "sold_NYT", 0, 1000, null, "INTEGER"]
      ]
    },
    "unsold": {
      "CLASS": "CONSTRAINT",
      "TYPE": "LINEAR",
      "KEYS": ["title"],
      "RECIPE": {
        "Name": "unsold",
        "Sense": "GE",
        "Dual":  null,
        "Formula": ["MINUS", "order","sold"],
        "RHS": 0.0
      }
    },
    "profit": {
      "CLASS": "OBJECTIVE",
      "TYPE": "LINEAR",
      "SINGLETON": {
        "Name": "profit",
        "Row": "profit",
        "Type": "LINEAR",
        "Constant": 0.0,
        "Sense": "MAXIMIZE",
        "Value": null
      }
    },
    "profit_gas": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "SINGLETON": {
        "Type": "LINEAR",
        "Row": "total",
        "Column": "gas",
        "Entry": 40.0
      }
    },
    "profit_chloride": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "SINGLETON": {
        "Type": "LINEAR",
        "Row": "total",
        "Column": "chloride",
        "Entry": 50.0
      }
    },
    "ctMaxTotal_gas": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "SINGLETON": {
        "Type": "LINEAR",
        "Row": "ctMaxTotal",
        "Column": "gas",
        "Entry": 1.0
      }
    },
    "ctMaxTotal_chloride": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "SINGLETON": {
        "Type": "LINEAR",
        "Row": "ctMaxTotal",
        "Column": "chloride",
        "Entry": 1.0
      }
    },
    "ctMaxTotal2_gas": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "SINGLETON": {
        "Type": "LINEAR",
        "Row": "ctMaxTotal2",
        "Column": "gas",
        "Entry": 3.0
      }
    },
    "ctMaxTotal2_chloride": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "SINGLETON": {
        "Type": "LINEAR",
        "Row": "ctMaxTotal2",
        "Column": "chloride",
        "Entry": 4.0
      }
    },
    "ctMaxChloride_chloride": {
      "CLASS": "TERM",
      "TYPE": "LINEAR",
      "SINGLETON": {
        "Type": "LINEAR",
        "Row": "ctMaxChloride",
        "Column": "chloride",
        "Entry": 1.0
      }
    },
    "solution": {
      "CLASS": "DATA",
      "TYPE": "OUTPUT",
      "SINGLETON": {
        "gas": "#Solver.getValue('gas')",
        "chloride": "#Solver.getValue('chloride')",
        "profit": "#Solver.getObjValue()"
      }
    }
  }  }
