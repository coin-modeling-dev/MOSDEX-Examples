//@formatter:off
{
  "generalTransshipment": {
    "CLASS": "PROBLEM",
    "HEADING": {
      "DESCRIPTION": [
        "General Transshipment Problem",
        "recipe form, revised data format, new grammar"
      ],
      "VERSION": "New v 1.0 a",
      "REFERENCE": "https://ampl.com/BOOK/EXAMPLES/EXAMPLES2/net1.mod",
      "AUTHOR": "Jeremy A. Bloom (jeremyblmca@gmail.com)",
      "NOTICES": "Copyright Â© 2019 Jeremy A. Bloom",
      "EXPRESSION": [
        "var Ship {(i,j) in LINKS} >= 0, <= capacity[i,j]; # packages to be shipped",
        "minimize Total_Cost: sum {(i,j) in LINKS} cost[i,j] * Ship[i,j];",
        "subject to",
          "Balance {k in CITIES}: ",
            "sum {(k,j) in LINKS} Ship[k,j] - sum {(i,k) in LINKS} Ship[i,k] = supply[k] - demand[k];"
      ]
    },
    "cities": {
      "CLASS": "DATA",
      "ROLE": "INPUT",
      "SCHEMA": {
        "city": "STRING",
        "supply": "DOUBLE",
        "demand": "DOUBLE"
      },
      "FIELDS": ["city", "supply",  "demand"],
      "INSTANCE": [
        [        "PITT",  450.0,     0.0    ],
        [        "NE",    0.0,       0.0    ],
        [        "SE",    0.0,       0.0    ],
        [        "BOS",   0.0,       90.0   ],
        [        "EWR",   0.0,       120.0  ],
        [        "BWI",   0.0,       120.0  ],
        [        "ATL",   0.0,       70.0   ],
        [        "MCO",   0.0,       50.0   ]
      ]
    },
    "links": {
      "CLASS": "DATA",
      "ROLE": "INPUT",
      "SCHEMA": {
        "origin": "STRING",
        "destination": "STRING",
        "cost": "DOUBLE",
        "capacity": "DOUBLE"
      },
      "FIELDS": ["origin",  "destination",  "cost", "capacity"],
      "INSTANCE": [
        [         "PITT",    "NE",           2.5,    250.0     ],
        [         "PITT",    "SE",           3.5,    250.0     ],
        [         "NE",      "BOS",          1.7,    100.0     ],
        [         "NE",      "EWR",          0.7,    100.0     ],
        [         "NE",      "BWI",          1.3,    100.0     ],
        [         "SE",      "EWR",          1.3,    100.0     ],
        [         "SE",      "BWI",          0.8,    100.0     ],
        [         "SE",      "ATL",          0.2,    100.0     ],
        [         "SE",      "MCO",          2.1,    100.0     ]
      ]
    },
    "ship": {
      "CLASS": "VARIABLE",
      "ROLE": "OPTIMIZATION",
      "RECIPE":{
        "SELECT": [
          "'ship' AS Name",
          "'CONTINUOUS' AS Type",
          "origin", "destination",
          "CONCAT('ship','_', origin,'_',destination) AS Column ", 
          "0.0 AS LowerBound",
          "capacity AS UpperBound",
          "NULL AS Value"
        ],
        "FROM": ["links"]
      }
    },
    "balance": {
      "CLASS": "CONSTRAINT",
      "ROLE": "OPTIMIZATION",
      "RECIPE":{
        "SELECT": [
          "'balance' AS Name",
          "'LINEAR' AS Type",
          "city",
          "CONCAT('balance','_',city) AS Row", 
          "'EQ' AS Sense",
          "(supply-demand) AS RHS"
        ],
        "FROM": ["cities"]
      }
    },
    "totalCost": {
      "CLASS": "OBJECTIVE",
      "ROLE": "OPTIMIZATION",
      "SINGLETON": {
        "Name":"totalCost",
        "Row": "totalCost",
        "Type": "LINEAR",
        "Constant": 0.0,
        "Sense": "MINIMIZE",
        "Value": null
      }
    },
    "balance_shipFrom": {
      "CLASS": "COEFFICIENT",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": ["balance.Row AS Row", "ship.Column AS Column", "1.0 AS Entry"], 
        "FROM": ["balance"],
        "JOIN":["JOIN ship", "ON (balance.city == ship.origin)"]
      }
    },
    "balance_shipTo": {
      "CLASS": "COEFFICIENT",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": ["balance.Row AS Row", "ship.Column AS Column", "-1.0 AS Entry"], 
        "FROM": ["balance"],
        "JOIN":["JOIN ship", "ON (balance.city == ship.destination)"]
      }
    },
    "total_ship": {
      "CLASS": "COEFFICIENT",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": ["totalCost.Row AS Row", "ship.Column AS Column", "links.cost AS Entry"],
        "FROM": ["totalCost"],
        "JOIN": [
          "JOIN ship",
          "JOIN links", "ON (links.origin == ship.origin AND links.destination == ship.destination)"
        ]
      }
    },
    "shipments": {
      "CLASS": "DATA",
      "ROLE": "OUTPUT",
      "RECIPE": {
        "SELECT": ["origin", "destination", "Value AS shipment"],
        "FROM": ["ship"]
      }
    },
    "objective": {
      "CLASS": "DATA",
      "ROLE": "OUTPUT",
      "RECIPE": {
        "SELECT": ["Value AS TotalCost"], 
        "FROM": ["totalCost"]
      }
    }
  }
}

