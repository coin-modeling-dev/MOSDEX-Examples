{
  "trafficNetworkQP": {
    "CLASS": "PROBLEM",
    "HEADING": {
      "DESCRIPTION": "Quadratically constrained network problem",
      "REFERENCE": "https://ampl.com/MEETINGS/TALKS/2015_11_Philadelphia_MC19.pdf",
      "VERSION": "1.0",
      "AUTHOR": "Jeremy A. Bloom (jeremyblmca@gmail.com)",
      "NOTICES": "Copyright Â© 2019 Jeremy A. Bloom",
      "EXPRESSION": [
        "var Flow {(i,j) in ROADS} >= 0, <= .9999 * cap[i,j];",
        "var Delay{ROADS} >= 0;",
        "minimize Avg_Time:sum {(i,j) in ROADS} (base[i,j]*Flow[i,j] + Delay[i,j]) / throughput;",
        "subject to Delay_Def {(i,j) in ROADS}:",
        "   sensitivity[i,j] * Flow[i,j]^2 +Flow[i,j]/capacity[i,j] * Delay[i,j] -Delay[i,j] <= 0.0;",
        "subject to Balance_Node {i in INTERS}:",
        "   sum{(i,j) in ROADS} Flow[i,j] -sum{(j,i) in ROADS} Flow[j,i] = 0.0;",
        "subject to Balance_Enter:",
        "   sum{(parameters.entrance,j) in ROADS} Flow[parameters.entrance,j] = throughput"
      ]
    },
    "roads": {
      "CLASS": "DATA",
      "ROLE": "INPUT",
      "SCHEMA": {
        "origin": "STRING",
        "destination": "STRING",
        "base": "DOUBLE",
        "capacity": "DOUBLE",
        "sensitivity": "DOUBLE"
      },
      "FIELDS": ["origin", "destination", "base", "capacity", "sensitivity"],
      "INSTANCE": [
        [        "a",      "b",           4.0,    10.0,       0.1],
        [        "a",      "c",           1.0,    12.0,       0.7],
        [        "c",      "b",           2.0,    20.0,       0.9],
        [        "b",      "d",           1.0,    15.0,       0.5],
        [        "c",      "d",           6.0,    10.0,       0.1]
      ]
    },
    "parameters": {
      "CLASS": "DATA",
      "ROLE": "INPUT",
      "SINGLETON": {
        "entrance": "a",
        "exit": "d",
        "throughput": 20.0}
    },
    "flow": {
      "CLASS": "VARIABLE",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": [
          "'flow' AS Name",
          "'CONTINUOUS' AS Type",
          "origin",
          "destination",
          "CONCAT('flow','_', origin,'_', destination) AS Column",
          "0.0 As LowerBound",
          "0.999* capacity AS UpperBound",
          "null AS Value"
        ],
        "FROM": ["roads"]
      }
    },
    "delay": {
      "CLASS": "VARIABLE",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": [
          "'delay' AS Name",
          "'CONTINUOUS' AS Type",
          "origin",
          "destination",
          "CONCAT('delay','_', origin,'_', destination) AS Column",
          "0.0 As LowerBound",
          "'infinity' AS UpperBound",
          "null AS Value"
        ],
        "FROM": ["roads"]
      }
      
    },
    "avgTime": {
      "CLASS": "OBJECTIVE",
      "ROLE": "OPTIMIZATION",
      "SINGLETON": {
        "Name": "avgTime",
        "Type": "LINEAR",
        "Row": "avgTime",
        "Constant": 0.0,
        "Sense":"MINIMIZE",
        "Value": null
      }
    },
    "avgTime_flow": {
      "CLASS": "COEFFICIENT",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": {
          "SELECT": [
            "'avgTime' AS Row",
            "flow.Column AS Column",
            "roads.base/parameters.throughput AS Entry"],
          "FROM": ["flow", "parameters"],
          "JOIN": ["JOIN roads", "USING (origin, destination)"]
        }
      }
    },
    "avgTime_delay": {
      "CLASS": "COEFFICIENT",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": {
          "SELECT": [
            "'avgTime' AS Row",
            "delay.Column AS Column",
            "-1.0 AS Entry"],
          "FROM": ["delay", "parameters"]
        }
      }
    },
    "delayDef": {
      "CLASS": "CONSTRAINT",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": [
          "'delayDef' AS Name",
          "origin",
          "destination",
          "CONCAT('delayDef','_', origin,'_', destination) AS Row",
          "'QUADRATIC' AS Type",
          "'LE' AS Sense",
          "0.0 AS RHS"],
        "FROM": ["roads"]
      }
    },
      "delayDef_flowSq": {
        "CLASS": "COEFFICIENT",
        "ROLE": "OPTIMIZATION",
        "RECIPE": {
          "SELECT": {
            "SELECT": [
              "delayDef.Row AS Row",
              "flow.Column AS Column1",
              "flow.Column AS Column2",
              "roads.sensitivity AS Entry"],
            "FROM": ["delayDef"],
            "JOIN": [
              "JOIN roads", "USING (origin, destination)",
              "JOIN flow",  "USING (origin, destination)"
            ]
          }
        }
      },
      "delayDef_flow_delay": {
        "CLASS": "COEFFICIENT",
        "ROLE": "OPTIMIZATION",
        "RECIPE": {
          "SELECT": {
            "SELECT": [
              "delayDef.Row AS Row",
              "flow.Column AS Column1",
              "delay.Column AS Column2",
              "1.0/roads.capacity AS Entry"],
            "FROM": ["delayDef"],
            "JOIN": [
              "JOIN flow",  "USING (origin, destination)",
              "JOIN delay", "USING (origin, destination)",
              "JOIN roads", "USING (origin, destination)"
            ]
          }
        }
      },
      "delayDef_delay": {
        "CLASS": "COEFFICIENT",
        "ROLE": "OPTIMIZATION",
        "RECIPE": {
          "SELECT": {
            "SELECT": [
              "delayDef.Row AS Row",
              "delay.Column AS Column",
              "-1.0 AS Entry"],
            "FROM": ["delayDef"],
            "JOIN": [
              "JOIN delay", " USING (origin, destination)"
            ]
          }
        }
      },
    
    "balanceNode": {
      "CLASS": "CONSTRAINT",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": [
          "'balanceNode' AS Name",
          "origin",
          "CONCAT('balanceNode','_', origin) AS Row",
          "'LINEAR' AS Type",
          "'EQ' AS Sense",
          "0.0 AS RHS"],
        "FROM": ["roads"]
      }
    },
      "balanceNode_flowOut": {
        "CLASS": "COEFFICIENT",
        "ROLE": "OPTIMIZATION",
        "RECIPE": {
          "SELECT": [
            "balanceNode.Row AS Row",
            "flow.Column AS Column",
            "1.0 AS Entry"],
          "FROM": ["balanceNode"],
          "JOIN": ["flow", "USING origin"]
        }
      },
      "balanceNode_flowIn": {
        "CLASS": "COEFFICIENT",
        "ROLE": "OPTIMIZATION",
        "RECIPE": {
          "SELECT": [
            "balanceNode.Row AS Row",
            "flow.Column AS Column",
            "-1.0 AS Entry"],
          "FROM": ["balanceNode"],
          "JOIN": ["flow", "ON (balanceNode.origin = flow.destination)"]
        }
      },
    "balanceEnter": {
      "CLASS": "CONSTRAINT",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": [
          "'balanceEnter' AS Name",
          "parameters.entrance AS origin",
          "CONCAT('balanceEnter') AS Row",
          "'LINEAR' AS Type",
          "'EQ' AS Sense",
          "parameters.throughput AS RHS"],
        "FROM": ["parameters"]
      }
    },
    "balanceEnter_flowOut": {
      "CLASS": "COEFFICIENT",
      "ROLE": "OPTIMIZATION",
      "RECIPE": {
        "SELECT": [
          "balanceEnter.Row AS Row",
          "flow.Column AS Column",
          "1.0 AS Entry"],
        "FROM": ["balanceEnter"],
        "JOIN": ["flow", "USING origin"]
      }
    }
  }
}